<div class="fields">
<table width=100% border=1 cellspacing=0 BORDERCOLOR="#C0C0C0 " BORDERSTYLE="solid">
	<td width=10%><%= f.text_field :timing, :size => 5 %> MIN</td>
	<td width=30%><%= f.text_area :objective, :size => '30x3' %>  </td>
	<td width=45%><%= f.text_area :task, :size => '40x4' %></td>
	<td width=10%><%= f.text_field :tool, :size => 9 %></td><%= f.hidden_field :_destroy %>
	<td width=5%><%= link_to_remove_fields image_tag("delete.png", :border => 0, :title => 'Remove'), f %></td>
</table>
</div>